	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Sikora, J and David-Uraz, A and Chowdhury, S and Bowman, D M and Wade, G A and Khalack, V and Kobzar, O and Kochukhov, O and Neiner, C and Paunzen, E},
	month = aug,
	year = {2019},
