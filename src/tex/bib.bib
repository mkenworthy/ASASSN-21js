	month = jun,
	year = {2011},
	note = {Publisher: EDP Sciences},
	pages = {A146},
	file = {Full Text PDF:C\:\\Users\\Hobba\\Zotero\\storage\\TFIIN6CQ\\Chadima et al. - 2011 - Spectral and photometric analysis of the eclipsing.pdf:application/pdf},
}

